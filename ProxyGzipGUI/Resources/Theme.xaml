<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="BackgroundColor">#ff002b36</Color>
    <Color x:Key="InputColor">#ff859900</Color>
    <Color x:Key="OutputColor">#ff268bd2</Color>
    <SolidColorBrush x:Key="BackgroundBrush" Color="{DynamicResource BackgroundColor}"/>
    <SolidColorBrush x:Key="InputBrush" Color="{DynamicResource InputColor}"/>
    <SolidColorBrush x:Key="OutputBrush" Color="{DynamicResource OutputColor}"/>
    <FontFamily x:Key="DataFont">Consolas</FontFamily>
</ResourceDictionary>